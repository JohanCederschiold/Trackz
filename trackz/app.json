{
  "expo": {
    "name": "Treazures",
    "slug": "trackz",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "se.jpdc.Treazures",
      "permissions" : ["ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION", "LOCATION"]
    }
  }
}
